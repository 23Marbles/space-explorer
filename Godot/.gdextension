[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://.lib/debug/librust.so"
linux.release.x86_64 =   "res://.lib/release/librust.so"
windows.debug.x86_64 =   "res://.lib/debug/rust.dll"
windows.release.x86_64 = "res://.lib/release/rust.dll"
macos.debug =            "res://.lib/debug/librust.dylib"
macos.release =          "res://.lib/release/librust.dylib"
macos.debug.arm64 =      "res://.lib/debug/librust.dylib"
macos.release.arm64 =    "res://.lib/release/librust.dylib"
